[core]
  editor = vim
[mergetool "meld"]
	cmd = meld
	trustExitCode = true
[merge]
	tool = meld
[github]
	user = allynfolksjr
[color]
	ui = auto
	status = auto
	branch = auto
	interactive = auto
	diff = auto
[alias]
  b = branch
  co = checkout
  ci = commit
  dc = diff --cached
  st = status
  pu = remote update --prune
  lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
  ll = log --graph --all --format=oneline --decorate


